{ 
  "version": 2, 
  "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts",
      "methods": ["GET", "HEAD", "PUT", "PATCH", "POST", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
